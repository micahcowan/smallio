{
    "compilerOptions": {
        "target": "es5"
      , "strictNullChecks": true
      , "noImplicitAny": true
      , "sourceMap": true
      , "declaration": true
      , "outDir": "build/"
      , "rootDir": "src/"
      , "moduleResolution": "Node"
      , "listEmittedFiles": true
      , "allowUnreachableCode": true
    }
  , "include": [
        "src/**.ts"
    ]
}
